{
  "type": "module",
  "dependencies": {
    "express": "^5.1.0",
    "googleapis": "^164.1.0",
    "path": "^0.12.7"
  },
  "scripts": {
    "start-server": "cross-env NODE_DEBUG=express node server.js",
    "watch-css": "npx tailwindcss -i ./public/input.css -o ./public/output.css --watch",
    "dev": "concurrently \"npm run start-server\" \"npm run watch-css\"",
    "photographer": "cross-env PROJECT_NAME=photographer npm run dev", 
    "neurodiv": "cross-env PROJECT_NAME=neurodiv npm run dev",
    "upload": "node cloudinary-upload.js",
    "folders": "node img-folder-to-json.js",
    "cld-upload": "cld upload_dir about -e -f about -p blackmac_preset -o overwrite true"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.21",
    "cloudinary": "^2.8.0",
    "concurrently": "^9.2.1",
    "cross-env": "^7.0.3",
    "dotenv": "^17.2.3",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.18"
  }
}
